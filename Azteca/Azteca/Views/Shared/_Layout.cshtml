@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="gA Jobs Site">
    <meta name="author" content="Grupo ASSA">
    <title>@ViewBag.Title</title>

    <script src="~/Content/Bootstrap/js/jquery.min.js"></script>
    <link href="~/Content/Bootstrap/css/bootstrap.min.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/Bootstrap/css/bootstrap-theme.min.css" type="text/css" rel="stylesheet" />
    <script src="~/Content/Bootstrap/js/bootstrap.min.js"></script>
    <link href="~/Content/Theme/main.css" type="text/css" rel="stylesheet" />
</head>
<body>
    <!-- HEADER BOOTSTRAP -->

    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index","Home")"><img src="~/Content/Images/Flags/ga.jpg" /></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="@ViewBag.ICOHome"><a href="@Url.Action("Index","Home")"><span class="glyphicon glyphicon-home"></span></a></li>
                    <li class="@ViewBag.ICOHacemos"><a href="#">¿Qué hacemos?</a></li>
                    <li class="@ViewBag.ICOCV"><a href="@Url.Action("Form","Carga")">Ingresá tu CV</a></li>
                    <li class="nav-divider"></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Lenguaje <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li class="active"><a href="#"><img src="~/Content/Images/Flags/Argentina.png" /> Español</a></li>
                            <li><a href="#"><img src="~/Content/Images/Flags/Brazil.png" /> Portuges</a></li>
                            <li><a href="#"><img src="~/Content/Images/Flags/Usa.png" /> English</a></li>
                        </ul>
                    </li>
                </ul>
            </div><!--/.navbar-collapse -->
        </div>
    </div>


    <!-- END HEADER BOOTSTRAP -->
    <div id="content">
        @RenderBody()
    </div>
    <div id="footer">
    
    </div>
</body>
</html>
